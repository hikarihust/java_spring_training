<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="appleCategory" class="spring.ex.AppleCategory"></bean>
	<bean id="samsungCategory" class="spring.ex.SamsungCategory"></bean>
	<bean id="service" class="spring.ex.CategoryService">
		<constructor-arg index="0" ref="samsungCategory"></constructor-arg>
	</bean>
</beans>
